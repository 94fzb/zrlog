{
  "name": "blog-web-frontend",
  "version": "0.1.0",
  "homepage": "./install",
  "private": true,
  "dependencies": {
    "antd": "4.21.3",
    "axios": "0.27.2",
    "history": "5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0"
  },
  "scripts": {
    "lint": "yarn type-check",
    "prepare": "cd ../../../../ && husky install blog-web/src/main/frontend/.husky",
    "pre-commit": "yarn lint",
    "start": "craco start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "build": "cross-env GENERATE_SOURCEMAP=false craco build",
    "test": "craco test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "node": "16.15.1",
    "@craco/craco": "6.4.3",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-loadable": "5.5.6",
    "@types/react-router": "5.1.18",
    "@types/react-router-dom": "5.3.2",
    "@typescript-eslint/typescript-estree": "5.29.0",
    "craco-less": "2.0.0",
    "cross-env": "7.0.3",
    "react-loadable": "5.5.0",
    "react-scripts": "5.0.1",
    "typescript": "4.5.4",
    "husky": "7.0.4",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings=0",
      "git add"
    ]
  }
}
